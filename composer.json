{
    "name": "greenplugin/telegram-bot-api",
    "type": "library",
    "description": "this is low low lwvwl telegram bot api",
    "keywords": [
        "greenplugin",
        "telegram-bot-api"
    ],
    "homepage": "https://github.com/greenplugin/telegram-bot-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Greenplugin",
            "email": "wformps@gmail.com",
            "homepage": "https://github.com/Greenplugin/telegram-bot-api",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">7.2",
        "kriswallsmith/buzz": "^0.17.2",
        "symfony/property-access": "^4.2",
        "symfony/property-info": "^4.2",
        "symfony/serializer": "^4.2",
        "ext-json": "*"
    },
    "require-dev": {
        "phpro/grumphp": "^0.14.3",
        "phpunit/phpunit": "~7.5.1",
        "squizlabs/php_codesniffer": "^3.0",
        "phpstan/phpstan": "^0.10.6",
        "povils/phpmnd": "^2.0",
        "sebastian/phpcpd": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Greenplugin\\TelegramBot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Greenplugin\\TelegramBot\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
